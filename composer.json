		"thefox/logic": "^0.1",
		"thefox/network": "^0.1"
	},
	"require-dev": {
		"symfony/finder": "^2.5",
		"phpunit/phpunit": "^6.2",
		"doctrine/common": "^2.4",
		"phpstan/phpstan": "^0.7",
